{
  "project": "iain-mag / dabc-training",
  "editUrlLabel": "Document Authoring",
  "editUrlPattern": "https://da.live/edit#/{owner}/{repo}/{path}",
  "plugins": [
    {
      "id": "da-plugin-demo",
      "title": "Plugin demo",
      "environments": ["edit"],
      "daLibrary": false,
      "url": "https://main--dabc-training--iain-mag.hlx.live/tools/demo/demo.html"
    },
    {
      "id": "experimentation",
      "titleI18n": { "en": "Experimentation" },
      "environments": ["dev", "preview"],
      "event": "experimentation"
    },
    {
      "id": "aem-labs",
      "title": "AEM Labs",
      "environments": ["edit"],
      "daLibrary": false,
      "experience": "window",
      "url": "https://labs.aem.live"
    },
    {
      "id": "asset-library",
      "title": "My Assets",
      "environments": ["edit","preview"],
      "url": "https://experience.adobe.com/solutions/CQ-helix-assets-addon/static-assets/resources/asset-selector.html",
      "isPalette": true,
      "includePaths": ["**/*.docx"],
      "passConfig": true,
      "paletteRect": "top: 50px; bottom: 10px; right: 10px; left: auto; width:400px; height: calc(100vh - 60px)"
    }
    {
      "id": "library",
      "title": "Library",
      "environments": ["edit"],
      "url": "/tools/sidekick/library.html",
      "includePaths": ["**/*.docx"]
    }
  ],
  "apps": [
    {
      "title": "App demo",
      "description": "This is a sample app using the DA SDK.",
      "url": "https://da.live/app/iain-mag/dabc-training/tools/demo-app"
    },
    {
      "title": "Tag browser",
      "description": "Manage your tags",
      "url": "https://da.live/app/iain-mag/dabc-training/tools/tags"
    }
  ]
}
